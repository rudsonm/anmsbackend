    <file>/Vendors/angular/angular-route.min.js</file>

    <file>/Vendors/underscore/underscore.min.js</file>
    <file>/Vendors/angular/restangular.min.js</file>

    <file>/Vendors/angular-material/angular-material.min.js</file>
    <file>/Vendors/angular-material/md-data-table.min.js</file>        
    
    <file>/Vendors/jquery/jquery.min.js</file>
    <file>/Vendors/jquery/jquery.mask.min.js</file>

    <file>/Vendors/linq/linq.js</file>    
